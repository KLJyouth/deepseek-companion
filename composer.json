{
    "name": "deepseek/companion",
    "license": "proprietary",
    "version": "1.0.0",
    "description": "智能文档自动化增强系统",
    "authors": [
        {
            "name": "DeepSeek Team",
            "email": "dev@deepseek.com"
        }
    ],
    "require": {
        "ext-posix": "*",
        "php": ">=8.1",
        "ext-json": "*",
        "ext-redis": "*",
        "monolog/monolog": "^2.8",
        "symfony/process": "^6.4",
        "endroid/qr-code": "^4.8",
        "ext-pdo": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "traits\\": "traits/",
            "Services\\": "services/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Services\\Installer:createDeploymentUser"
        ],
        "post-install-cmd": [
            "Services\\Installer:createDeploymentUser"
        ],
        "validate": "./scripts/validate_composer.sh"
    },
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}